{
  "name": "stormbraining",
  "version": "1.0.0",
  "description": "social brainstorming app",
  "engines": {
    "node": "4.1.1"
  },
  "scripts": {
    "clean": "rimraf client/dist",
    "build": "webpack --verbose --colors --display-error-details --config client/webpack/common.config.js",
    "build:production": "npm run clean && npm run build",
    "lint": "eslint --ignore-path .gitignore **/*.js",
    "postinstall": "npm run build:production",
    "start": "nodemon server/bin/server.js",
    "test": "npm run testFront",
    "testAll": "npm run testFront && npm run testBack",
    "testFront": "mocha client/src/test/**/*.spec.js --compilers js:babel-register --recursive",
    "testBack": "mocha server/test --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghostmoose/stormbraining.git"
  },
  "homepage": "https://github.com/ghostmoose/stormbraining#readme",
  "bugs": {
    "url": "https://github.com/ghostmoose/stormbraining/issues"
  },
  "license": "MIT",
  "authors": [
    "Kevin Chen (https://github.com/kevn-chn)",
    "Frances Swiecki-Bernhardt (https://github.com/fswiecki)",
    "Sara Gee (https://github.com/saragee3)",
    "David Nguyen (https://github.com/lllNuggetslll)"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
